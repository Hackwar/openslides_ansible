worker_processes 1;

events {
  worker_connections  1024;
}

http {
  include mime.types;
  default_type application/octet-stream;
  sendfile on;
  keepalive_timeout 65;

  {% include 'nginx_upstream.j2' %}

  server {
    listen 0.0.0.0:{{ openslides_nginx_port }};
    #listen [::]:{{ openslides_nginx_port}};

    {% include 'nginx_locations.j2' %}
  }
}
